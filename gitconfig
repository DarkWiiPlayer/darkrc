[core]
	editor = vim -f
[diff]
	tool = vimdiff
[difftool]
	confirm = false
	prompt = true
[merge]
	tool = vimdiff3
[push]
	default = matching
[alias]
	adog = log --all --decorate --oneline --graph
  ten = log --all --decorate --oneline --graph -10
[commit]
	gpgSign = true
